{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "jsx-no-lambda": false,
        "object-literal-sort-keys": false,
        "interface-name": false,
        "semicolon": [
            true,
            "never"
        ],
        "quotemark": [
            true,
            "single"
        ]
    },
    "rulesDirectory": []
}